{% extends "../partials/layout.njk" %}
{% from "govuk/components/table/macro.njk" import govukTable %}
{% from "../macros/printLink.njk" import printLink %}

{% set title = 'Establishment roll' %}

{% block beforeContent %}
  {{ govukBreadcrumbs({
    items: [
      {
        text: "Digital Prison Services",
        href: "/"
      }
    ],
    classes: 'govuk-!-display-none-print'
  })
  }}
{% endblock %}

{% block content %}
  <div class="govuk-body govuk-!-margin-bottom-9">
    <h1 class="govuk-heading-l ">The Establishment roll has moved</h1>
    <p>You can view the updated establishment roll at its <a class="govuk-link" href="{{ redirectUrl }}">new web address</a>.</p>
    <p>You should update any bookmarks to the new address.</p>
    <p>Find out more about the <a class="govuk-link" href="{{ changesUrl }}">changes we've made to the Establishment roll</a>.</p>
  </div>
{% endblock %}