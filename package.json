{
  "name": "prisonstaffhub",
  "version": "0.0.1",
  "private": false,
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "bunyan-middleware": "^0.8.0",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "govuk-elements-sass": "^3.1.2",
    "govuk_frontend_toolkit": "^7.5.0",
    "helmet": "^3.12.1",
    "hsts": "^2.1.0",
    "jsonwebtoken": "^8.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "dotenv-expand": "^4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "babel-eslint": "^8.2.1",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-react": "^7.8.2",
    "eslint": "^4.19.1",
    "file-loader": "^1.1.11",
    "fs": "^0.0.1-security",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "node-sass": "^4.9.0",
    "mkdirp": "^0.5.1",
    "raf": "^3.4.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-ga": "^2.5.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "path": "^0.12.7",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.1"
  },
  "scripts": {
    "start": "node backend/index.js",
    "start-prod": "NODE_ENV=production node backend/index.js",
    "build": "webpack --mode=production",
    "test": "jest --env=jsdom",
    "test-watch": "node scripts/test.js --env=jsdom --watch",
    "record-build-info": "node scripts/record-build-info",
    "plant-beanstalk": "node scripts/plant-beanstalk.js",
    "lint": "node node_modules/eslint/bin/eslint.js src backend"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/(src|backend)/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
