  "scripts": {
    "start": "node backend/index.js",
    "start:dev": "nodemon --watch backend --watch views backend/index.js",
    "start-feature": "npm run compile-sass && export $(cat $npm_config_env) && node backend/index.js",
    "start-feature-no-webpack": "export $(cat $npm_config_env) && DISABLE_WEBPACK=true node backend/index.js",
    "build": "webpack --mode=production && npm run compile-sass",
    "build:analyse": "BUNDLE_ANALYSE=true npm run build",
    "test": "jest --env=jsdom",
    "test-watch": "jest --env=jsdom --watch",
    "record-build-info": "node scripts/record-build-info",
    "lint": "node node_modules/eslint/bin/eslint.js src backend",
    "verify": "npm run lint && npm test",
    "security_audit": "npx audit-ci --config audit-ci.json",
    "int-test": "cypress run",
    "int-test-no-artefacts": "cypress run --config video=false,screenshotOnRunFailure=false",
    "int-test-ui": "cypress open",
    "compile-sass": "sass --load-path=. ./sass:./build/static/stylesheets --style compressed",
    "compile-sass-watch": "npm run compile-sass && sass --load-path=. ./sass:./build/static/stylesheets --watch --style compressed"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "backend/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "globalSetup": "<rootDir>/config/jest/globalSetup.js",
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/integration-tests/"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!(new-nomis-shared-components|spin.js)/).+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,json,css}": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": [
      "package.json"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      ".circleci/*",
      "migrations/*",
      "seeds/*",
      "node_modules/*",
      "integration-tests/*"
    ],
    "delay": 2500,
    "ext": "js,njk"
  }
}
