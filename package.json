  "scripts": {
    "start": "DISABLE_WEBPACK=true node dist/backend/index.js --unhandled-rejection=warn",
    "start:dev": "nodemon",
    "start-feature": "npm run compile-sass && export $(cat cypress.env) && npm run start:dev",
    "start-feature-no-webpack": "export $(cat cypress.env) && npm run build && npm run start",
    "build": "rm -rf dist && npm run build:frontend && npm run build:backend && npm run build:copy",
    "build:backend": "tsc",
    "build:frontend": "webpack --mode=production && npm run compile-sass",
    "build:copy": "cp -R views dist/views && cp -R build dist/build",
    "build:analyse": "BUNDLE_ANALYSE=true npm run build",
    "test": "jest --env=jsdom",
    "test-watch": "jest --env=jsdom --watch",
    "record-build-info": "node scripts/record-build-info",
    "lint": "node node_modules/eslint/bin/eslint.js src backend",
    "verify": "npm run lint && npm test",
    "security_audit": "npx audit-ci --config audit-ci.json",
    "int-test": "cypress run",
    "int-test-no-artefacts": "cypress run --config video=false,screenshotOnRunFailure=false",
    "int-test-ui": "cypress open",
    "compile-sass": "sass --load-path=. ./sass:./build/static/stylesheets --style compressed",
    "compile-sass-watch": "npm run compile-sass && sass --load-path=. ./sass:./build/static/stylesheets --watch --style compressed",
    "prepare": "husky install"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "backend/**/*.{js,jsx,mjs,ts,tsx}",
      "backend/**/*.{js,jsx,mjs,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "globalSetup": "<rootDir>/config/jest/globalSetup.js",
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/integration-tests/",
      "<rootDir>/dist/"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs|ts|tsx)$": "babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!(spin.js)/).+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs",
      "ts",
      "d.ts",
      "tsc"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  },
  "lint-staged": {
    "*.{ts,js,json,css}": [
      "prettier --write"
    ],
    "*.{ts,js}": [
      "eslint --cache --fix"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "backend",
      "common"
    ],
    "ext": "ts,d.ts,tsx,json,js,jsx,mjs",
    "ignore": [
      "**/*.test.*"
    ],
    "exec": "ts-node ./backend/index.ts | bunyan -o simple"
  }
}
